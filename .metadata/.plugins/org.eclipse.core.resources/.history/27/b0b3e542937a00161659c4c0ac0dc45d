package general;

public class Celda {

	protected double x;
	protected double y;
	protected String path;
	
	public Celda(int x,int y){
		this.x=x;
		this.y=y;
		path="Vacio.png";
	}
	
	public Celda(int x2, int y2, String ruta) {
		x=x2;
		y=y2;
		path=ruta;
	}

	public double getX(){
		return x;
	}
	
	public double getY(){
		return y;
	}
	
	public void moverX(double x){
		this.x+=x;
	}
	
	public void moverY(double d){
		this.y+=d;
	}
	
	public String getPath(){
		return path;
	}
	
	public void setPath(String ruta){
		path=ruta;
	}

	
}
