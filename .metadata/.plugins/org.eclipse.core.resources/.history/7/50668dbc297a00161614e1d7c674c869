	{
		 recompensa=400;
		 velocidadMov=1;
		 velocidadDisp=2;
		 ubicacion=u;
		 aguante=4;
		// contador=0;
		 direccion=2;
	}
	
	
	@Override
	public Proyectil disparar() {
		// TODO Auto-generated method stub
		return null;
	}

	
	public Celda getUbicacion() {
		return ubicacion;
	}

}
